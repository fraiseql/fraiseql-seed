{
  "name": "fraiseql-seed",
  "version": "0.1.0",
  "description": "Auto-resolving test data for PostgreSQL - UUID patterns and data generation",
  "private": true,
  "scripts": {
    "lint:md": "markdownlint '**/*.md' --ignore node_modules --ignore .venv --ignore .pytest_cache --ignore .phases",
    "check:links": "find . -name '*.md' ! -path './.*' ! -path '*/node_modules/*' ! -path '*/.venv/*' ! -path '*/.pytest_cache/*' ! -path '*/.phases/*' -exec markdown-link-check --config .github/markdown-link-check.json {} \\;"
  },
  "devDependencies": {
    "markdown-link-check": "^3.12.1",
    "markdownlint-cli": "^0.39.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fraiseql/fraiseql-seed"
  },
  "author": "Lionel Hamayon <lionel.hamayon@evolution-digitale.fr>",
  "license": "MIT"
}
